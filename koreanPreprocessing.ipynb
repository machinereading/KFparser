   "source": [
    "import json\n",
    "def gen_lufrmmap(word):\n",
    "    with open('./KFN_lus.json','r') as f:\n",
    "        kofrn = json.load(f)\n",
    "    \n",
    "    for i in kofrn:\n",
    "        lu = i['lu'].split('.')[0]\n",
    "        if word == lu:\n",
    "            result = i['frameName']\n",
    "            break\n",
    "    return result\n",
    "        \n",
    "    "
   ]
  }
